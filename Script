new-project 100 100

add-layer pokadots-blue

set-filter pokadots-blue blue

add-image-to-layer 0 0 res/pokadots.png pokadots-blue 

add-layer pokadots-dark

add-image-to-layer 0 0 res/pokadots.png pokedots-dark 

set-filter boxy-dark darken-luma

add-layer pokadots-multiply

add-image-to-layer 0 0 res/pokadots.png pokedots-multiply 

set-filter pokadots-dark multiply

save-image res/pokadots-pokadots.ppm

save-project projects/pokadots-pokadots.ppm


